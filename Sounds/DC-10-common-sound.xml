		<misc>
			<name>dc10 avionics</name>
			<mode>looped</mode>
			<path>FL2070/11_efis.wav</path>
			<condition>
				<or>
					<greater-than-equals>
						<property>systems/electrical/bus/ac1</property>
						<value>110</value>
					</greater-than-equals>
					<greater-than-equals>

					</greater-than-equals>
					<greater-than-equals>
						<property>systems/electrical/bus/ac3</property>
						<value>110</value>
					</greater-than-equals>
				</or>
			</condition>
			
			<volume>
				<property>sim/sound/internal-custom</property>
				<factor>1.4</factor>
				<offset>0.1</offset>
			</volume>
		</misc>
		

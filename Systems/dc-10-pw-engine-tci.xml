		<!-- Overspeed Alert -->
		<fcs_function name="engine/limit/overspeed-epr-input">
			<function>
				<sum>
					<max>
						<property>engine/epr-actual-1</property>
						<property>engine/epr-actual-2</property>
						<property>engine/epr-actual-3</property>
					</max>
					<value>-0.03</value>
				</sum>
			</function>
		</fcs_function>
		
		<switch name="engine/limit/overspeed">
			<default value="0"/>
			<test value="1">
				engine/limit/overspeed-epr-input gt engine/limit/highest
			</test>
		</switch>
	

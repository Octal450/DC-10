			<input>engine/reverse-3/position-cmd</input>
			<rate_limit>engine/reverse-3/position-rate</rate_limit>
			<output>/engines/engine[2]/reverser-pos-norm</output>
		</actuator>
		
		<pure_gain name="propulsion/engine[2]/reverser-angle-rad">
			<input>engine/reverse-3/position-norm</input>
			<gain>3.14</gain>
		</pure_gain>
		
		<switch name="engine/reverse-3/interlock"> <!-- Locked until 60% open, unlocked until 20% closed -->
			<default value="engine/reverse-3/interlock"/>
			<test value="0">
				/engines/engine[2]/reverser-pos-norm ge 0.6
			</test>
			<test value="1">
				/engines/engine[2]/reverser-pos-norm le 0.2
			</test>
		</switch>
		
		<fcs_function name="engine/reverse-3/throttle-rev-cmd">
			<function>
				<ifthen>
					<eq>
						<property>engine/reverse-3/position-norm</property>
						<value>1</value>
					</eq>
					<table>
						<independentVar lookup="row">/controls/engines/engine[2]/throttle</independentVar>
						<tableData>
							0.05  0.00
							1.00  0.82
						</tableData>
					</table>
					<value>0</value>
				</ifthen>
			</function>
		</fcs_function>
		
		<fcs_function name="/controls/engines/engine[2]/reverse-lever">
			<function>
				<product>
					<property>/controls/engines/engine[2]/reverse-engage</property>
					<table>
						<independentVar lookup="row">engine/reverse-3/throttle-rev-cmd</independentVar>
						<tableData>
							0.05  0.25
							0.82  1.00
						</tableData>
					</table>
				</product>
			</function>
		</fcs_function>
		
		<switch name="engine/reverse-3/throttle-rev">
			<default value="0"/>
			<test value="0">
				engine/reverse-3/interlock eq 1
			</test>
			<test value="engine/reverse-3/throttle-rev-cmd">
				/controls/engines/engine[2]/reverse-engage eq 1
			</test>
		</switch>
		
		<switch name="engine/reverse-3/active">
			<default value="0"/>
			<test logic="OR" value="1">
				engine/reverse-3/position-cmd eq 1
				engine/reverse-3/position-norm ne 0
			</test>
		</switch>
	
	</channel>
	
	<channel name="Engine Control">
		
		<!-- Engine 1 -->
		<fcs_function name="engine/limit/man-1-norm">
			<function>
				<table>
					<independentVar lookup="row">/controls/engines/engine[0]/man-n1</independentVar>
					<tableData>
						 22.6  0
						119.5  1
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<switch name="engine/control-1/ats-max">
			<default value="engine/limit/active-norm"/>
			<test logic="AND" value="engine/limit/man-1-norm">
				/instrumentation/n[0]/power-avail eq 1
				/controls/engines/engine[0]/man-n1-set eq 1
				engine/limit/man-1-norm le engine/limit/active-norm
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<switch name="engine/control-1/throttle-pos">
			<default value="/controls/engines/engine[0]/throttle"/>
			<test value="0">
				engine/reverse-1/active eq 1
			</test>
			<test logic="AND" value="engine/ats-cmd-1">
				/it-autoflight/output/athr eq 1
				/it-autoflight/output/thr-mode ne 3
			</test>
		</switch>
		
		<lag_filter name="engine/control-1/throttle-pos-lag">
			<input>engine/control-1/throttle-pos</input>
			<c1>20</c1>
		</lag_filter>
		
		<switch name="engine/control-1/throttle-output">
			<default value="engine/control-1/throttle-pos"/>
			<test value="engine/reverse-1/throttle-rev">
				engine/reverse-1/active eq 1
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<lag_filter name="engine/control-1/throttle-fdm">
			<input>engine/control-1/throttle-output</input>
			<c1>1.0</c1>
			<output>fcs/throttle-pos-norm[0]</output>
		</lag_filter>
		
		<!-- Engine 2 -->
		<fcs_function name="engine/limit/man-2-norm">
			<function>
				<table>
					<independentVar lookup="row">/controls/engines/engine[1]/man-n1</independentVar>
					<tableData>
						 22.6  0
						119.5  1
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<switch name="engine/control-2/ats-max">
			<default value="engine/limit/active-norm"/>
			<test logic="AND" value="engine/limit/man-2-norm">
				/instrumentation/n[1]/power-avail eq 1
				/controls/engines/engine[1]/man-n1-set eq 1
				engine/limit/man-2-norm le engine/limit/active-norm
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<switch name="engine/control-2/throttle-pos">
			<default value="/controls/engines/engine[1]/throttle"/>
			<test value="0">
				engine/reverse-2/active eq 1
			</test>
			<test logic="AND" value="engine/ats-cmd-2">
				/it-autoflight/output/athr eq 1
				/it-autoflight/output/thr-mode ne 3
			</test>
		</switch>
		
		<lag_filter name="engine/control-2/throttle-pos-lag">
			<input>engine/control-2/throttle-pos</input>
			<c1>20</c1>
		</lag_filter>
		
		<switch name="engine/control-2/throttle-output">
			<default value="engine/control-2/throttle-pos"/>
			<test value="engine/reverse-2/throttle-rev">
				engine/reverse-2/active eq 1
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<lag_filter name="engine/control-2/throttle-fdm">
			<input>engine/control-2/throttle-output</input>
			<c1>1.0</c1>
			<output>fcs/throttle-pos-norm[1]</output>
		</lag_filter>
		
		<!-- Engine 3 -->
		<fcs_function name="engine/limit/man-3-norm">
			<function>
				<table>
					<independentVar lookup="row">/controls/engines/engine[2]/man-n1</independentVar>
					<tableData>
						 22.6  0
						119.5  1
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<switch name="engine/control-3/ats-max">
			<default value="engine/limit/active-norm"/>
			<test logic="AND" value="engine/limit/man-3-norm">
				/instrumentation/n[2]/power-avail eq 1
				/controls/engines/engine[2]/man-n1-set eq 1
				engine/limit/man-3-norm le engine/limit/active-norm
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<switch name="engine/control-3/throttle-pos">
			<default value="/controls/engines/engine[2]/throttle"/>
			<test value="0">
				engine/reverse-3/active eq 1
			</test>
			<test logic="AND" value="engine/ats-cmd-3">
				/it-autoflight/output/athr eq 1
				/it-autoflight/output/thr-mode ne 3
			</test>
		</switch>
		
		<lag_filter name="engine/control-3/throttle-pos-lag">
			<input>engine/control-3/throttle-pos</input>
			<c1>20</c1>
		</lag_filter>
		
		<switch name="engine/control-3/throttle-output">
			<default value="engine/control-3/throttle-pos"/>
			<test value="engine/reverse-3/throttle-rev">
				engine/reverse-3/active eq 1
			</test>
			<clipto>
				<min>engine/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</switch>
		
		<lag_filter name="engine/control-3/throttle-fdm">
			<input>engine/control-3/throttle-output</input>
			<c1>1.0</c1>
			<output>fcs/throttle-pos-norm[2]</output>
		</lag_filter>
		
		<!-- Comparator -->
		<fcs_function name="engine/throttle-compare-max">
			<function>
				<max> <!-- Don't use clipped -->
					<property>engine/control-1/throttle-pos-lag</property>
					<property>engine/control-2/throttle-pos-lag</property>
					<property>engine/control-3/throttle-pos-lag</property>
				</max>
			</function>
		</fcs_function>
	
	</channel>
	
	<channel name="Engine Parameters" execrate="2">
		
		<lag_filter name="engine/n1-actual-1">
			<input>/engines/engine[0]/n1</input>
			<c1>2.25</c1>
			<output>/engines/engine[0]/n1-actual</output>
			<output>/engines/engine[4]/n1</output>
		</lag_filter>
		
		<lag_filter name="engine/n1-actual-2">
			<input>/engines/engine[1]/n1</input>
			<c1>2.25</c1>
			<output>/engines/engine[1]/n1-actual</output>
			<output>/engines/engine[5]/n1</output>
		</lag_filter>
		
		<lag_filter name="engine/n1-actual-3">
			<input>/engines/engine[2]/n1</input>
			<c1>2.25</c1>
			<output>/engines/engine[2]/n1-actual</output>
			<output>/engines/engine[6]/n1</output>
		</lag_filter>
		
		<switch name="engine/egt-c1">
			<default value="0.35"/>
			<test value="1000">
				/systems/acconfig/autoconfig-running eq 1
			</test>
		</switch>
		
		<fcs_function name="engine/egt-cmd-1">
			<function>
				<sum>
					<property>propulsion/tat-c</property>
					<table>
						<independentVar lookup="row">/engines/engine[0]/n2</independentVar>
						<independentVar lookup="column">atmosphere/density-altitude</independentVar>
						<independentVar lookup="table">/controls/engines/engine[0]/cutoff</independentVar>
						<tableData breakPoint="0">
							         0    50000
							 10.0    0    0
							 35.0  540  430
							 50.0  535  425
							 61.8  420  295
							109.5  895  670
						</tableData>
						<tableData breakPoint="1">
							       0    50000
							 30.0  0    0
							 61.8  420  295
							109.5  895  670
						</tableData>
					</table>
				</sum>
			</function>
		</fcs_function>
		
		<lag_filter name="engine/egt-actual-1">
			<input>engine/egt-cmd-1</input>
			<c1>engine/egt-c1</c1>
			<output>/engines/engine[0]/egt-actual</output>
		</lag_filter>
		
		<fcs_function name="engine/egt-cmd-2">
			<function>
				<sum>
					<property>propulsion/tat-c</property>
					<table>
						<independentVar lookup="row">/engines/engine[1]/n2</independentVar>
						<independentVar lookup="column">atmosphere/density-altitude</independentVar>
						<independentVar lookup="table">/controls/engines/engine[1]/cutoff</independentVar>
						<tableData breakPoint="0">
							         0    50000
							 10.0    0    0
							 35.0  540  430
							 50.0  535  425
							 61.8  420  295
							109.5  895  670
						</tableData>
						<tableData breakPoint="1">
							       0    50000
							 30.0  0    0
							 61.8  420  295
							109.5  895  670
						</tableData>
					</table>
				</sum>
			</function>
		</fcs_function>
		
		<lag_filter name="engine/egt-actual-2">
			<input>engine/egt-cmd-2</input>
			<c1>engine/egt-c1</c1>
			<output>/engines/engine[1]/egt-actual</output>
		</lag_filter>
		
		<fcs_function name="engine/egt-cmd-3">
			<function>
				<sum>
					<property>propulsion/tat-c</property>
					<table>
						<independentVar lookup="row">/engines/engine[2]/n2</independentVar>
						<independentVar lookup="column">atmosphere/density-altitude</independentVar>
						<independentVar lookup="table">/controls/engines/engine[2]/cutoff</independentVar>
						<tableData breakPoint="0">
							         0    50000
							 10.0    0    0
							 35.0  540  430
							 50.0  535  425
							 61.8  420  295
							109.5  895  670
						</tableData>
						<tableData breakPoint="1">
							       0    50000
							 30.0  0    0
							 61.8  420  295
							109.5  895  670
						</tableData>
					</table>
				</sum>
			</function>
		</fcs_function>
		
		<lag_filter name="engine/egt-actual-3">
			<input>engine/egt-cmd-3</input>
			<c1>engine/egt-c1</c1>
			<output>/engines/engine[2]/egt-actual</output>
		</lag_filter>
		
		<lag_filter name="engine/n2-actual-1">
			<input>/engines/engine[0]/n2</input>
			<c1>2.25</c1>
			<output>/engines/engine[0]/n2-actual</output>
			<output>/engines/engine[4]/n2</output>
		</lag_filter>
		
		<lag_filter name="engine/n2-actual-2">
			<input>/engines/engine[1]/n2</input>
			<c1>2.25</c1>
			<output>/engines/engine[1]/n2-actual</output>
			<output>/engines/engine[5]/n2</output>
		</lag_filter>
		
		<lag_filter name="engine/n2-actual-3">
			<input>/engines/engine[2]/n2</input>
			<c1>2.25</c1>
			<output>/engines/engine[2]/n2-actual</output>
			<output>/engines/engine[6]/n2</output>
		</lag_filter>
		
		<lag_filter name="engine/ff-actual-1">
			<input>/engines/engine[0]/fuel-flow_pph</input>
			<c1>3.0</c1>
			<output>/engines/engine[0]/ff-actual</output>
		</lag_filter>
		
		<lag_filter name="engine/ff-actual-2">
			<input>/engines/engine[1]/fuel-flow_pph</input>
			<c1>3.0</c1>
			<output>/engines/engine[1]/ff-actual</output>
		</lag_filter>
		
		<lag_filter name="engine/ff-actual-3">
			<input>/engines/engine[2]/fuel-flow_pph</input>
			<c1>3.0</c1>
			<output>/engines/engine[2]/ff-actual</output>
		</lag_filter>
		
		<!-- Overspeed Alert -->
		<fcs_function name="engine/limit/overspeed-n1-input">
			<function>
				<sum>
					<max>
						<property>engine/n1-actual-1</property>
						<property>engine/n1-actual-2</property>
						<property>engine/n1-actual-3</property>
					</max>
					<value>-2</value>
				</sum>
			</function>
		</fcs_function>
		
		<switch name="engine/limit/overspeed">
			<default value="0"/>
			<test value="1">
				engine/limit/overspeed-n1-input gt engine/limit/highest
			</test>
		</switch>
	
	</channel>
	
	<channel name="Reverser Status" execrate="8">
		
		<switch name="engine/eng-1-rev-state">
			<default value="0"/>
			<test value="3"> <!-- REV -->
				engine/reverse-1/position-norm eq 1
			</test>
			<test logic="OR" value="2"> <!-- U/L -->
				/controls/engines/engine[0]/reverse-engage eq 1
				engine/reverse-1/position-norm gt 0.2
			</test>
			<test logic="OR" value="1"> <!-- PRES -->
				/controls/engines/engine[0]/reverse-engage eq 1
				engine/reverse-1/position-norm gt 0
			</test>
		</switch>
		
		<switch name="engine/eng-2-rev-state">
			<default value="0"/>
			<test value="3"> <!-- REV -->
				engine/reverse-2/position-norm eq 1
			</test>
			<test logic="OR" value="2"> <!-- U/L -->
				/controls/engines/engine[1]/reverse-engage eq 1
				engine/reverse-2/position-norm gt 0.2
			</test>
			<test logic="OR" value="1"> <!-- PRES -->
				/controls/engines/engine[1]/reverse-engage eq 1
				engine/reverse-2/position-norm gt 0
			</test>
		</switch>
		
		<switch name="engine/eng-3-rev-state">
			<default value="0"/>
			<test value="3"> <!-- REV -->
				engine/reverse-3/position-norm eq 1
			</test>
			<test logic="OR" value="2"> <!-- U/L -->
				/controls/engines/engine[2]/reverse-engage eq 1
				engine/reverse-3/position-norm gt 0.2
			</test>
			<test logic="OR" value="1"> <!-- PRES -->
				/controls/engines/engine[2]/reverse-engage eq 1
				engine/reverse-3/position-norm gt 0
			</test>
		</switch>
	
	</channel>

</system>

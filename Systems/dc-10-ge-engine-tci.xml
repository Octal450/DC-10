		<!-- Following functions fix a FGTurbine inaccuracy... rated thrust N1% should be rated thrust lbs -->
		<!-- These calculations change it so that the rated power at sea level is at the correct N1% value. It still changes with altitude and mach as it should -->
		<fcs_function name="engine/limit/fgturbine-thrust">
			<function>
				<sum>
					<product>
						<property>propulsion/engine[0]/IdleThrust</property> <!-- It doesn't matter which engine, same for all -->
						<property>engine/limit/rated-thrust</property>
					</product>
					<product>
						<difference>
							<property>engine/limit/rated-thrust</property>
							<product>
								<property>propulsion/engine[0]/IdleThrust</property> <!-- It doesn't matter which engine, same for all -->
								<property>engine/limit/rated-thrust</property>
							</product>
						</difference>
						<property>engine/limit/milthrust-unmodified</property>
					</product>
				</sum>
			</function>
		</fcs_function>
		
		<fcs_function name="engine/limit/rated-thrust-actual">
			<function>
				<sum>
					<product>
						<property>propulsion/engine[0]/IdleThrust</property> <!-- It doesn't matter which engine, same for all -->
						<property>engine/limit/rated-thrust</property>
					</product>
					<product>
						<product>
							<difference>
								<property>engine/limit/rated-thrust</property>
								<product>
									<property>propulsion/engine[0]/IdleThrust</property> <!-- It doesn't matter which engine, same for all -->
									<property>engine/limit/rated-thrust</property>
								</product>
							</difference>
							<property>engine/limit/milthrust-unmodified</property>
						</product>
						<pow>
							<quotient>
								<difference>
									<property>engine/limit/rated-thrust-n1</property>
									<value>22.6</value>
								</difference>
								<value>96.9</value>
							</quotient>
							<value>2</value>
						</pow>
					</product>
				</sum>
			</function>
		</fcs_function>
		
		<fcs_function name="engine/limit/rated-thrust-factor">
			<function>
				<ifthen>
					<eq>
						<property>/systems/acconfig/autoconfig-running</property>
						<value>1</value>
					</eq>
					<value>0</value>
					<ifthen>
						<nq> <!-- Prevent divide by 0 -->
							<property>engine/limit/rated-thrust-actual</property>
							<value>0</value>
						</nq>
						<quotient>
							<property>engine/limit/fgturbine-thrust</property>
							<property>engine/limit/rated-thrust-actual</property>
						</quotient>
						<value>1</value>
					</ifthen>
				</ifthen>
			</function>
		</fcs_function>
		

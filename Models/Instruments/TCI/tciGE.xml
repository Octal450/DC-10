	<animation>
		<type>material</type>
		<object-name>LightingGroup</object-name>
		<emission>
			<red>1.0</red>
			<green>0.6</green>
			<blue>0.3</blue>
			<factor-prop>systems/lighting/center-panel</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>lim.flag</object-name>
		<object-name>tat.flag</object-name>
		<condition>
			<not><property>instrumentation/tci/power-avail</property></not>
		</condition>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>to.btn</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<equals>
						<property>instrumentation/tci/power-avail</property>
						<value>1</value>
					</equals>
				</condition>
				<command>nasal</command>
				<script>systems.THRLIM.setMode(0);</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/cockpit/tci/to</property>
				<value>1</value>
			</binding>
			<binding>
				<command>nasal</command>
				<script>libraries.Sound.btn1();</script>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/cockpit/tci/to</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	

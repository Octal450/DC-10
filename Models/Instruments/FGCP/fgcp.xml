	<!-- ATS Switches -->
	<animation>
		<type>pick</type>
		<object-name>Ats1</object-name>
		<object-name>Ats2</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<button>3</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/athr</property>
			</binding>
			<binding>
				<condition>
					<less-than>
						<property>controls/cockpit/fgcp/ats</property>
						<value>0.5</value>
					</less-than>
				</condition>
				<command>property-assign</command>
				<property>controls/cockpit/fgcp/ats-sw</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/cockpit/fgcp/ats-sw</property>
					<value>0</value>
				</binding>
			</mod-up>
			<binding>
				<command>nasal</command>
				<script>libraries.Sound.switch1();</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>Ats1</object-name>
		<object-name>Ats2</object-name>
		<property>controls/cockpit/fgcp/ats1</property>
		<factor>0.03554</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<!-- Speed Knob -->
	<animation>
		<type>pick</type>
		<object-name>Spd.knob</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>controls/cockpit/fgcp/spd-translate</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/cockpit/fgcp/spd-translate</property>
					<value>0</value>
				</binding>
			</mod-up>
			<binding>
				<command>nasal</command>
				<script>cockpit.ApPanel.spdPull();</script>
			</binding>
		</action>
		<action>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>nasal</command>
				<script>cockpit.ApPanel.spdAdjust(1);</script>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>nasal</command>
				<script>cockpit.ApPanel.spdAdjust(10);</script>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/spd-rotate</property>
				<step>1</step>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
		<action>
			<button>4</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>nasal</command>
				<script>cockpit.ApPanel.spdAdjust(-1);</script>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>nasal</command>
				<script>cockpit.ApPanel.spdAdjust(-10);</script>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/spd-rotate</property>
				<step>-1</step>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	

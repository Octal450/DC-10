	<!-- Speed Controls -->
	<animation>
		<type>pick</type>
		<object-name>Epr</object-name>
		<object-name>N1</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					cockpit.ApPanel.eprN1();

		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Hsi2</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<button>3</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>instrumentation/hsi[1]/slaved-to-gps</property>
			</binding>
			<binding>
				<command>nasal</command>
				<script>libraries.Sound.switch1();</script>
			</binding>
		</action>
	</animation>
	
	<!-- NAV1 Freq Window -->
	<animation>
		<type>textranslate</type>
		<object-name>Nav1.hundreds</object-name>
		<property>instrumentation/nav[0]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.00001</factor>
		<step>10000</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav1.tens</object-name>
		<property>instrumentation/nav[0]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav1.ones</object-name>
		<property>instrumentation/nav[0]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav1.tenths</object-name>
		<property>instrumentation/nav[0]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav1.hundreths</object-name>
		<property>instrumentation/nav[0]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.1</factor>
		<step>1</step>
		<bias>0.1</bias>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	
	<!-- NAV1 Freq Knobs -->
	<animation>
		<type>knob</type>
		<object-name>Nav1Knob1</object-name>
		<property>controls/cockpit/fgcp/nav1-major-rotate</property>
		<factor>-11.25</factor>
		<center>
			<x-m>0</x-m>
			<y-m>-0.26451</y-m>
			<z-m>0.00815</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav[0]/frequencies/selected-mhz</property>
				<factor>1</factor>
				<min>108</min>
				<max>118</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/nav1-major-rotate</property>
				<factor>1</factor>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>Nav1Knob2</object-name>
		<property>controls/cockpit/fgcp/nav1-minor-rotate</property>
		<factor>-11.25</factor>
		<center>
			<x-m>0</x-m>
			<y-m>-0.26451</y-m>
			<z-m>0.00815</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<increase>
			<binding>
				<command>nasal</command>
				<script>instruments.NAV.adjustDecimal(0, 1);</script>
			</binding>
		</increase>
		<decrease>
			<binding>
				<command>nasal</command>
				<script>instruments.NAV.adjustDecimal(0, -1);</script>
			</binding>
		</decrease>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/nav1-minor-rotate</property>
				<factor>1</factor>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	

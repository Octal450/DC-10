		<object-name>Ap1</object-name>
		<property>controls/cockpit/fgcp/ap1</property>
		<factor>0.01777</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Ap2</object-name>
		<action>
			<button>0</button>
			<button>3</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					cockpit.ApPanel.apFgs2(1);
					libraries.Sound.switch1();
				</script>
			</binding>
			<binding>
				<condition>
					<less-than>
						<property>controls/cockpit/fgcp/ap2</property>
						<value>0.5</value>
					</less-than>
				</condition>
				<command>property-assign</command>
				<property>controls/cockpit/fgcp/ap2-sw</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/cockpit/fgcp/ap2-sw</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
		<action>
			<button>1</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					cockpit.ApPanel.apFgs2(-1);
					libraries.Sound.switch1();
				</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/cockpit/fgcp/ap2-sw</property>
				<value>0</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>Ap2</object-name>
		<property>controls/cockpit/fgcp/ap2</property>
		<factor>0.01777</factor>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>
	
	<!-- NAV2 Freq Window -->
	<animation>
		<type>textranslate</type>
		<object-name>Nav2.hundreds</object-name>
		<property>instrumentation/nav[1]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.00001</factor>
		<step>10000</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav2.tens</object-name>
		<property>instrumentation/nav[1]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav2.ones</object-name>
		<property>instrumentation/nav[1]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav2.tenths</object-name>
		<property>instrumentation/nav[1]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>Nav2.hundreths</object-name>
		<property>instrumentation/nav[1]/frequencies/selected-mhz-fmt-x100</property>
		<factor>0.1</factor>
		<step>1</step>
		<bias>0.1</bias>
		<axis>
			<x>0</x>
			<y>1</y>
		</axis>
	</animation>
	
	<!-- NAV2 Freq Knobs -->
	<animation>
		<type>knob</type>
		<object-name>Nav2Knob1</object-name>
		<property>controls/cockpit/fgcp/nav2-major-rotate</property>
		<factor>-11.25</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0.25065</y-m>
			<z-m>0.00815</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/nav[1]/frequencies/selected-mhz</property>
				<factor>1</factor>
				<min>108</min>
				<max>118</max>
				<wrap>true</wrap>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/nav2-major-rotate</property>
				<factor>1</factor>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>knob</type>
		<object-name>Nav2Knob2</object-name>
		<property>controls/cockpit/fgcp/nav2-minor-rotate</property>
		<factor>-11.25</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0.25065</y-m>
			<z-m>0.00815</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<increase>
			<binding>
				<command>nasal</command>
				<script>instruments.NAV.adjustDecimal(1, 1);</script>
			</binding>
		</increase>
		<decrease>
			<binding>
				<command>nasal</command>
				<script>instruments.NAV.adjustDecimal(1, -1);</script>
			</binding>
		</decrease>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/fgcp/nav2-minor-rotate</property>
				<factor>1</factor>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	
	<!-- NAV2 Course Window -->
	<animation>
		<type>textranslate</type>
		<object-name>Nav2Crs.hundreds</object-name>
		<property>instrumentation/nav[1]/radials/selected-deg</property>
		<factor>0.001</factor>

<PropertyList>
	
	<path>fgcp.ac</path>
	
	<animation>
		<type>material</type>
		<object-name>Buttons</object-name>
		<object-name>LightingGroup</object-name>
		<emission>
			<red>0.6</red>
			<green>0.45</green>
			<blue>0.3</blue>
			<factor-prop>controls/lighting/main-panel-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>N1</object-name>
		<condition>
			<equals>
				<property>options/eng</property>
				<value>GE</value>
			</equals>
		</condition>
	</animation>
	
	<animation>
		<type>select</type>
		<object-name>Epr</object-name>
		<condition>
			<equals>
				<property>options/eng</property>
				<value>PW</value>
			</equals>
		</condition>
	</animation>
	
	<!-- Flight Director Switches -->
	<animation>
		<type>rotate</type>
		<object-name>Fd1</object-name>
		<property>controls/cockpit/fgcp/fd1</property>
		<interpolation>
			<entry><ind>0</ind><dep> 20</dep></entry>
			<entry><ind>1</ind><dep>-20</dep></entry>
		</interpolation>
		<center>
			<x-m>0.06404</x-m>
			<y-m>0</y-m>
			<z-m>0.04406</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Fd1</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<button>3</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/fd1</property>
			</binding>
			<binding>
				<command>nasal</command>
				<script>libraries.Sound.switch1();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>Fd2</object-name>
		<property>controls/cockpit/fgcp/fd2</property>
		<interpolation>
			<entry><ind>0</ind><dep> 20</dep></entry>
			<entry><ind>1</ind><dep>-20</dep></entry>
		</interpolation>
		<center>
			<x-m>0.06404</x-m>
			<y-m>0</y-m>
			<z-m>0.04406</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>Fd2</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<button>3</button>
			<button>4</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/fd2</property>
			</binding>
			<binding>
				<command>nasal</command>
				<script>libraries.Sound.switch1();</script>
			</binding>
		</action>
	</animation>
	
	<!-- Turb -->
	<animation>
		<type>pick</type>
		<object-name>Turb1</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
					cockpit.ApPanel.turb();
					libraries.Sound.btn1();
				</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>controls/cockpit/fgcp/turb-1</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/cockpit/fgcp/turb-1</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>Turb1</object-name>
		<property>controls/cockpit/fgcp/turb-1</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>Turb2</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>

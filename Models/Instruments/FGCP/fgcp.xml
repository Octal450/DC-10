	<animation>
		<type>translate</type>
		<object-name>Turb2</object-name>
		<property>controls/cockpit/fgcp/turb-2</property>
		<factor>-0.001</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	
	<!-- HSI 1 Switch -->

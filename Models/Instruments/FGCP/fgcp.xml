		<interpolation>
			<entry><ind>0</ind><dep>-20</dep></entry>
			<entry><ind>1</ind><dep> 20</dep></entry>
		</interpolation>
		<center>
			<x-m>0.06</x-m>
			<y-m>0</y-m>
			<z-m>-0.03677</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>

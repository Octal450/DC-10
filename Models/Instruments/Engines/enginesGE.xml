	<!-- Knobs -->
	<animation>
		<type>pick</type>
		<object-name>N1Cmd1.Knob</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/engines/engine[0]/man-n1-set</property>
			</binding>
		</action>
		<action>
			<button>3</button>
			<repeatable>true</repeatable>
			<binding>
				<condition>
					<not><property>devices/status/keyboard/shift</property></not>
				</condition>
				<command>nasal</command>
				<script>systems.ENGINE.adjustManN1(0, 1);</script>
			</binding>
			<binding>
				<condition>
					<property>devices/status/keyboard/shift</property>
				</condition>
				<command>nasal</command>
				<script>systems.ENGINE.adjustManN1(0, 10);</script>
			</binding>
			<binding>
				<command>property-adjust</command>
				<property>controls/cockpit/n1-cmd-rotate[0]</property>
				<step>1</step>
				<min>0</min>
				<max>100000</max>
				<wrap>true</wrap>
			</binding>
		</action>
		<action>

	<!-- Speedbrake/spoiler lever -->
	<animation>
		<type>pick</type>
		<object-name>SpeedbrakeLever</object-name>
		<action>
			<button>0</button>
			<button>1</button>
			<binding>
				<command>property-assign</command>
				<property>controls/flight/speedbrake</property>
				<value>0</value>
			</binding>
			<binding>
				<command>property-cycle</command>
				<property>controls/flight/speedbrake-arm</property>
				<value>0</value>
				<value>1</value>
			</binding>
		</action>
		<action>
			<button>3</button>
			<binding>
				<command>nasal</command>
				<script>libraries.retractSpeedbrake();</script>
			</binding>
		</action>
		<action>
			<button>4</button>
			<binding>
				<command>nasal</command>
				<script>libraries.deploySpeedbrake();</script>
			</binding>
		</action>
